@* @model List<Curso> *@

<table class="table table-striped table-hover">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Nombre</th>
            <th scope="col">Jornada</th>
            <th scope="col">Mantenimiento</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Id</td>
                <td>@obj.Nombre</td>
                <td>@obj.Jornada</td>
                <td>
                    <div>
                        @Html.ActionLink("Details", "Index", new { cursoId = obj.Id } , new { @class = "btn btn-outline-info"})
                        @Html.ActionLink("Edit", "Edit", new { cursoId = obj.Id} , new { @class = "btn btn-outline-primary"})
                        @Html.ActionLink("Delete", "Delete", new { cursoId = obj.Id} , new { @class = "btn btn-outline-danger"})
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>